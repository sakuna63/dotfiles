echo 'load .zshrc.antigen'

# ~/antigenフォルダ下の設定ファイルを読み込む
source ~/.zsh.d/packages/antigen/antigen.zsh

# oh-my-zshのライブラリをロードする
antigen-use oh-my-zsh

# syntax highliting
antigen bundle zsh-users/zsh-syntax-highlighting

# テーマを変更する（元に戻したいときはコメントアウトすればいい）
# https://github.com/robbyrussell/oh-my-zsh/wiki/Themes
#antigen-theme robbyrussell
antigen-theme af-magic

# antigenの終了(rubyでいうend???)
antigen-apply
